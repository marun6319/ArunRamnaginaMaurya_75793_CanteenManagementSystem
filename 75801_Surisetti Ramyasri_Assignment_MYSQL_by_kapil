23.)	select  title ,(price-discount) "FINAL PRICE" from BOOK;

24.)	select * from BOOK where price > 250 && discount is NOT NULL;

25.)	select publisher , pdate from BOOK where titile like 'a%' || pdate < '2017-01-01';

26.)	select title, publisher , pdate from BOOK where pdate IN('2018-07-01' , '2017-01-22' , '2015-12-31');

27.)	select bookid , title, publisher ,isb_no, pdate from BOOK where pdate IN('2018-06-01' , '2017-01-22' );

